{
  "name": "imi-svg-to-icon",
  "displayName": "IMI SVG to React Icon",
  "description": "Convert SVG files to React components using @imile/components SvgIcon",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:imi-svg-to-icon.convertSvg"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "imi-svg-to-icon.convertSvg",
        "title": "Convert SVG to React Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "imi-svg-to-icon.convertSvg",
          "when": "resourceExtname == .svg",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "imi-svg-to-icon.convertSvg",
          "when": "resourceExtname == .svg",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "imi-svg-to-icon.convertSvg",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}