{
  "name": "imi-svg-to-icon",
  "displayName": "IMI SVG to React Icon",
  "description": "Easily convert SVG files to React components with @imile/components SvgIcon. Right-click any SVG file to generate TypeScript React component instantly.",
  "version": "0.0.4",
  "publisher": "VincentChris",
  "license": "MIT",
  "keywords": [
    "svg",
    "react",
    "icon",
    "component",
    "converter",
    "imile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VincentChris/imi-svg-to-icon"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "imi-svg-to-icon.convertSvg",
        "title": "Convert SVG to React Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "imi-svg-to-icon.convertSvg",
          "when": "resourceExtname == .svg",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "imi-svg-to-icon.convertSvg",
          "when": "resourceExtname == .svg",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "imi-svg-to-icon.convertSvg",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^4.9.4"
  }
}
